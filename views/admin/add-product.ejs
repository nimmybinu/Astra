<%- include('../layouts/adminHeader.ejs') -%>



            <!-- Recent Sales Start -->
            <div class="col-sm-12 col-xl-6">
                
                <div class="bg-secondary rounded h-100 p-4">
                    <h6 class="mb-4">Add Product</h6>
                    <form action="" method="post" enctype="multipart/form-data" class="register-form">
                    <div class="form-floating mb-3">
                        <input type="text" name="pdtID" class="form-control" id="floatingInput"
                            placeholder="Enter pdtID" required>
                        <label for="floatingInput">ProductId</label>
                       
                    </div>
                    <div class="form-floating mb-3">
                        <input type="text" name="title" class="form-control" id="floatingTitle"
                            placeholder="Enter title" required>
                        <label for="floatingTitle">Title</label>
                    </div>
                    <div class="form-floating mb-3">
                        <input type="text" name="desc" class="form-control" id="floatingdesc"
                            placeholder="Enter Description" required>
                        <label for="floatingdesc">Description</label>
                    </div>
                    <div class="form-floating mb-3">
                        <input type="text" name="price" class="form-control" id="floatingPrice"
                            placeholder="Enter price" required>
                        <label for="floatingPrice">Price</label>
                    </div>
                    <div class="form-floating mb-3">
                        <input type="text" name="saleprice" class="form-control" id="floatingSalePrice"
                            placeholder="Enter saleprice" required>
                        <label for="floatingSalePrice">SalePrice</label>
                    </div>
                    <div class="form-floating mb-3">
                        <input type="text" name="category" class="form-control" id="floatingCategory"
                            placeholder="Enter category" required>
                        <label for="floatingCategory">Category</label>
                    </div>
                    <div class="form-floating mb-3">
                        <input type="text" name="stock" class="form-control" id="floatingStock"
                            placeholder="Enter stock" required>
                        <label for="floatingStock">Stock</label>
                    </div>
                    <div class="form-floating mb-3">
                        <input type="text" name="size" class="form-control" id="floatingSize"
                            placeholder="Enter size" required>
                        <label for="floatingSize">Size</label>
                    </div>
                    <div class="form-floating mb-3">
                        <input type="file" name="files" class="form-control" id="floatingImage"
                            placeholder="Enter size" required accept="image/*" multiple>
                        <label for="floatingImage">Image</label>
                    </div>
                    <button class="btn btn-sm btn-primary" type="submit" value="Add Product">Add Product</button>
                    </form>
                    <% if (typeof message !== 'undefined') { %>
                        <p style="color: green;"><%= message %></p>
                    <% } %>
                     
                </div>
            </div>
            <!-- Recent Sales End -->
            
            <a class="btn btn-sm btn-primary" href="/admin/dashboard/products">Back</a></td>
          

